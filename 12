import java.sql.Array;

public class Main {
    public static void main(String[] args) {

        Numbers(new int[20]);
    }

    public static void Numbers(int[] array) {
        for (int i = 0; i < array.length; i++) {
            if (array[i] < 6) {
                array[i] *= 2;
            }
        }
    }



}
